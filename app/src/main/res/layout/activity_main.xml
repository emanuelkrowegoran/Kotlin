<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <TextView
            android:id="@+id/nama"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true" android:layout_marginEnd="127dp" android:layout_marginTop="226dp"/>

    <TextView
            android:id="@+id/nim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.506"
            app:layout_constraintVertical_bias="0.54" android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true" android:layout_marginBottom="245dp"/>

    <ImageView
            android:layout_width="match_parent"
            android:layout_height="190dp"
            android:src="@drawable/ic_launcher_background" android:layout_alignParentStart="true"
            android:layout_alignParentTop="true" android:layout_marginTop="19dp"/>
</RelativeLayout>